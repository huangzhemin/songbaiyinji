<!--components/announcementList/announcementList.wxml-->
<block wx:for='{{announcementList}}' wx:for-index="idx" wx:key="idx">
  <pm-announcement-cell 
    avatar="{{item.avatar}}"
    nickName="{{item.nickName}}" 
    desc="{{item.desc}}"
    msgTime="{{item.msgTime}}" >
  </pm-announcement-cell>
</block>
